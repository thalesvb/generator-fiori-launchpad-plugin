{
  "name": "@thalesvb/generator-fiori-launchpad-plugin",
  "version": "0.0.0",
  "description": "Humble SAP Fiori Launchpad Plugin generator",
  "homepage": "https://github.com/thalesvb/generator-fiori-launchpad-plugin",
  "author": {
    "name": "Thales Batista",
    "email": "thalesvb@live.com",
    "url": "https://github.com/thalesvb"
  },
  "files": [
    "generators"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "sap",
    "fiori launchpad"
  ],
  "devDependencies": {
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^26.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "yeoman-generator": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "thalesvb/generator-fiori-launchpad-plugin",
  "license": "MIT"
}
